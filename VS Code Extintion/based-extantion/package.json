{
  "name": "based-extantion",
  "displayName": "Based Extantion",
  "description": "Based Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "Based",
      "aliases": ["Based", "Based"],
      "extensions": [".based"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "icon-based.png",
        "dark": "icon-based.png"
      }
    }],
    "grammars": [{
      "language": "Based",
      "scopeName": "source.based",
      "path": "./syntaxes/Based.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "Based",
        "scopeName": "source.based",
        "path": "./snippets/Based.Snippets.json"
      }
    ]
  }
}